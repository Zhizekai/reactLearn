State}>查看当前组件的props和store里的状态</button>\r\n                <ul>\r\n                    {users.map((item) => {\r\n                        console.log(\"render\",item);\r\n                        return (\r\n                            <li key={item.id}>姓名：{item.name}--年龄：{item.age}</li>\r\n                        )\r\n                    })}\r\n                </ul>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default connect(\r\n    state => ({users: state.rens}),\r\n    {createAddPersonAction}\r\n)(Person);","G:\\Project\\react_mymapp\\src\\page\\demo_08\\utils\\uuidGetter.js",[],{"ruleId":"229","severity":1,"message":"230","line":1,"column":8,"nodeType":"231","messageId":"232","endLine":1,"endColumn":13},{"ruleId":"229","severity":1,"message":"233","line":2,"column":8,"nodeType":"231","messageId":"232","endLine":2,"endColumn":16},{"ruleId":"229","severity":1,"message":"234","line":3,"column":8,"nodeType":"231","messageId":"232","endLine":3,"endColumn":11},{"ruleId":"235","replacedBy":"236"},{"ruleId":"237","replacedBy":"238"},{"ruleId":"235","replacedBy":"239"},{"ruleId":"237","replacedBy":"240"},{"ruleId":"229","severity":1,"message":"241","line":3,"column":8,"nodeType":"231","messageId":"232","endLine":3,"endColumn":12},{"ruleId":"229","severity":1,"message":"241","line":2,"column":8,"nodeType":"231","messageId":"232","endLine":2,"endColumn":12},{"ruleId":"229","severity":1,"message":"242","line":24,"column":15,"nodeType":"231","messageId":"232","endLine":24,"endColumn":18},{"ruleId":"235","replacedBy":"243"},{"ruleId":"237","replacedBy":"244"},{"ruleId":"229","severity":1,"message":"245","line":42,"column":15,"nodeType":"231","messageId":"232","endLine":42,"endColumn":19},{"ruleId":"229","severity":1,"message":"246","line":31,"column":17,"nodeType":"231","messageId":"232","endLine":31,"endColumn":22},{"ruleId":"229","severity":1,"message":"247","line":31,"column":34,"nodeType":"231","messageId":"232","endLine":31,"endColumn":41},{"ruleId":"229